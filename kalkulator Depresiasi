Private Sub HitungGarisLurus_Click()
GARLURBEBANDEPRESIASIPERTAHUN.Value = (Val(GARLURNILAIAWALASET.Value) - Val(GARLURNILAIRESIDU.Value)) / Val(GARLURESTIMASIUMURASET.Value)


End Sub
Private Sub HITUNGSALMEN_Click()
TINGKATGARLUR = 1 / Val(SALMENESTIMASIUMURASET.Value)
TINGKATDEPRESIASI = Val(SALMENFAKTORPENYUSUTAN.Value) * TINGKATGARLUR
SALMENNILAIBUKUNTAHUN.Value = Val(SALMENHARGAAWALASET.Value) * (1 - TINGKATDEPRESIASI) ^ Val(SALMENNILAIBUKUTAHUNKE.Value)

End Sub
Private Sub HITUNGSALMENG_Click()
TINGKATGARLUR = 1 / Val(SALMENGESTIMASIUMURASET.Value)
TINGKATDEPRESIASI = 2 * TINGKATGARLUR
SALMENGHASILNILAIBUKU.Value = Val(SALMENGHARGAAWALASET.Value) * (1 - TINGKATDEPRESIASI) ^ Val(SALMENGNILAIBUKUTAHUNKE.Value)


End Sub


Private Sub HITUNGSYD_Click()
NILAI = (Val(SYDESTIMASIUMURASET.Value) * (Val(SYDESTIMASIUMURASET.Value) + 1)) / 2

DEPRESIASITAHUN = Val(SYDESTIMASIUMURASET.Value) - Val(SYDDEPRESIASITAHUNKE.Value) + 1

SYDHASILDEPRESIASI.Value = (DEPRESIASITAHUN / NILAI) * (Val(SYDHARGAASET.Value) - Val(SYDNILAIRESIDU.Value))

End Sub


Private Sub HITUNGUNPROD_click()

UNPRODEPRESIASIPERUNIT.Value = (Val(UNPROHARGAAWALASET.Value) - Val(UNPRONILAIRESIDU.Value)) / Val(UNPROESPROTO.Value)
UNPROHASILDEPRESIASI.Value = Val(UNPRODEPRESIASIPERUNIT.Value) * Val(UNPROPRODAK.Value)



End Sub

Private Sub CommandButton1_click()
GARLURBEBANDEPRESIASIPERTAHUN.Value = Null
GARLURNILAIAWALASET.Value = Null
GARLURNILAIRESIDU.Value = Null
GARLURESTIMASIUMURASET.Value = Null
SALMENESTIMASIUMURASET.Value = Null
SALMENFAKTORPENYUSUTAN.Value = Null
SALMENNILAIBUKUNTAHUN.Value = Null
SALMENHARGAAWALASET.Value = Null
SALMENNILAIBUKUTAHUNKE.Value = Null
SALMENGESTIMASIUMURASET.Value = Null
SALMENGHARGAAWALASET.Value = Null
SALMENGNILAIBUKUTAHUNKE.Value = Null
SYDESTIMASIUMURASET.Value = Null
SYDDEPRESIASITAHUNKE.Value = Null
SYDHASILDEPRESIASI.Value = Null
SYDHARGAASET.Value = Null
SYDNILAIRESIDU.Value = Null
UNPRODEPRESIASIPERUNIT.Value = Null
UNPROHARGAAWALASET.Value = Null
UNPRONILAIRESIDU.Value = Null
UNPROESPROTO.Value = Null
UNPROHASILDEPRESIASI.Value = Null
End Sub
